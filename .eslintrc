{
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "semi": "off",
    "no-unused-vars": "off"
  }
}
